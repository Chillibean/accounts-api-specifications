{
  "description": "Trial Configurations reporting",
  "type": "object",
  "properties": {
    "hits": {
      "type": "number",
      "description": "The total number of rows",
      "example": 100
    },
    "page": {
      "type": "number",
      "description": "The current page number",
      "example": 1
    },
    "per_page": {
      "type": "number",
      "description": "Numeber of rows per page",
      "example": 50
    },
    "results": {
      "type": "array",
      "items": {
        "$ref": "./StudyProcedureTaskDefinition.json"
      }
    },
    "study_procedure_task_definitions": {
      "type": "array",
      "items": {
        "$ref": "./Rows.json"
      }
    },
    "trial_configuration": {
      "$ref": "./TrialConfiguration.json"
    }
  },
  "required": [
    "hits",
    "page",
    "per_page",
    "results",
    "study_procedure_task_definitions",
    "trial_configuration"
  ],
  "additionalProperties": false
}
