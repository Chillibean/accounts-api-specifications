{
  "description": "A Subject",
  "type": "object",
  "properties": {
    "id": {
      "type": "number",
      "description": "The id of account",
      "example": 1
    },
    "subject_number": {
      "type": "string",
      "description": "The subject number",
      "example": "B0001001"
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "description": "The date and time the subject was created",
      "example": "2020-01-01T00:00:00Z"
    },
    "current_study_event": {
      "type": "string",
      "description": "The current study event",
      "example": "REFILL1"
    },
    "study_arm": {
      "type": "string",
      "description": "The study arm",
      "example": "A"
    },
    "created_by": {
      "type": "string",
      "description": "The user of the subject",
      "example": "Mon Admin1"
    },
    "creator": {
      "$ref": "./User.json"
    }
  },
  "additionalProperties": false,
  "required": [
    "id",
    "subject_number",
    "current_study_event",
    "created_by",
    "user"
  ]
}