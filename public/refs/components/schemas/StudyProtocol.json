{
  "type": "object",
  "properties": {
    "id": {
      "type": "number",
      "description": "The id of the protocol",
      "example": 2
    },
    "name": {
      "type": "string",
      "description": "The name of a protocol",
      "example": "Alpha Protocol"
    },
    "study_event_display_name": {
      "type": "object",
      "description": "The plural and singular names of the study event",
      "example": {
        "plural": "refill visits",
        "singular": "refill visit"
      }
    },
    "study_procedure_display_name": {
      "type": "object",
      "description": "The plural and singular names of the study procedure",
      "example": {
        "plural": "procedures",
        "singular": "procedure"
      }
    },
    "locked": {
      "type": "boolean",
      "description": "Whether the protocol is locked",
      "example": true
    },
    "version_number": {
      "type": "number",
      "description": "The version number of the protocol",
      "example": 1
    },
    "study_procedure_role_definitions": {
      "type": "array",
      "description": "Study procedure role definitions belonging to the protocol",
      "items": {
        "$ref": "./StudyProcedureRoleDefinition.json"
      }
    }
  },
  "required": [
    "id",
    "name",
    "locked",
    "version_number",
    "study_procedure_role_definitions"
  ],
  "additionalProperties": false
}