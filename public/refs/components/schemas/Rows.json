{
  "description": "Reporting row",
  "type": "object",
  "properties": {
    "id": {
      "type": "number",
      "description": "The id of the protocol",
      "example": 1
    },
    "created_at": {
      "type": "string",
      "description": "The date the row was created",
      "example": "2020-01-01T00:00:00.000Z"
    },
    "identifier": {
      "type": "string",
      "description": "The indentifier of the protocol",
      "example": "NSTAR"
    },
    "display_name": {
      "type": "string",
      "description": "The display name of the protocol",
      "example": "North Star"
    },
    "date": {
      "type": "string",
      "description": "The protocol date",
      "example": "2020-01-01"
    },
    "status": {
      "type": "string",
      "description": "The protocol status",
      "example": "completed"
    },
    "status_change_date": {
      "type": "string",
      "description": "The protocol status change date",
      "example": "2022-08-10T10:00:00.000Z"
    },
    "clinical_evaluator": {
      "type": "string",
      "description": "The clinical evaluator",
      "example": "Mark Smith"
    },
    "account_trial_name": {
      "type": "string",
      "description": "The account trial",
      "example": "Trial 1"
    },
    "study_protocol_name": {
      "type": "string",
      "description": "Study protocol name",
      "example": "Protocol v1"
    },
    "site": {
      "$ref": "./Site.json"
    },
    "subject": {
      "type": "object",
      "properties": {
        "number": {
          "type": "string",
          "description": "The subject number",
          "example": "1234"
        },
        "arm": {
          "type": "string",
          "description": "The subject arm",
          "example": "Arm A",
          "nullable": true
        }
      }
    },
    "study_event": {
      "$ref": "./StudyEvent.json"
    },
    "flagged_tasks_raised": {
      "type": "string",
      "description": "Flagged tasks raised",
      "example": "yes"
    },
    "flagged_tasks": {
      "type": "array",
      "items": {
        "$ref": "./FlaggedTask.json"
      }
    },
    "tasks": {
      "type": "array",
      "items": {
        "$ref": "./Task.json"
      }
    },
    "completed_forms": {
      "type": "array",
      "items": {
        "$ref": "./CompletedForm.json"
      }
    },
    "assets": {
      "type": "array",
      "items": {
        "type": "integer",
        "example": 1
      }
    }
  },
  "required": [
    "id",
    "created_at",
    "identifier",
    "display_name",
    "date",
    "status",
    "status_change_date",
    "clinical_evaluator",
    "account_trial_name",
    "study_protocol_name",
    "site",
    "subject",
    "study_event",
    "flagged_tasks_raised",
    "flagged_tasks",
    "tasks",
    "completed_forms",
    "assets"
  ],
  "additionalProperties": false
}
