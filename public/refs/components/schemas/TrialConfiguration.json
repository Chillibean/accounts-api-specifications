      "type": "number",
      "description": "The id of the trial configuration",
      "example": 1
    },
    "name": {
      "type": "string",
      "description": "The name of the trial configuration",
      "example": "Trial Configuration v1"
    },
    "number_of_study_procedures": {
      "type": "number",
      "description": "The number of study procedures in the trial configuration",
      "example": 2
    },
    "study_procedure_display_name": {
      "$ref": "./DisplayName.json"
    },
    "study_event_display_name": {
      "$ref": "./DisplayName.json"
    }
  },
  "additionalProperties": false,
  "required": ["id", "name", "number_of_study_procedures"]
}
