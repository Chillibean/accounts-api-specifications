{
  "description": "An asset location",
  "type": "object",
  "properties": {
    "account": {
      "type": "object",
      "description": "The account data",
      "properties": {
        "name": {
          "type": "string",
          "description": "The name of the account",
          "example": "Test Trial"
        }
      },
      "additionalProperties": false,
      "required": [
        "name"
      ]
    },
    "site": {
      "type": "object",
      "description": "The site data",
      "properties": {
        "name": {
          "type": "string",
          "description": "The name of the site",
          "example": "0123 Hospital London St Thomas"
        },
        "number": {
          "type": "string",
          "description": "The number of the site",
          "example": 1
        }
      },
      "additionalProperties": false,
      "required": [
        "name",
        "number"
      ]
    },
    "subject": {
      "type": "object",
      "description": "The subject data",
      "properties": {
        "number": {
          "type": "string",
          "description": "The serial number of the subject",
          "example": "E001004"
        }
      },
      "additionalProperties": false,
      "required": [
        "number"
      ]
    },
    "study_event": {
      "type": "object",
      "description": "The study event data",
      "properties": {
        "display_name": {
          "type": "string",
          "description": "The display name of a study event",
          "example": "Visit 1"
        },
        "identifier": {
          "type": "string",
          "description": "The identifier of a study event",
          "example": "VIS1"
        },
        "label": {
          "$ref": "./DisplayName.json"
        }
      },
      "additionalProperties": false,
      "required": [
        "display_name",
        "identifier",
        "label"
      ]
    },
    "study_procedure": {
      "type": "object",
      "description": "The study procedure data",
      "properties": {
        "display_name": {
          "type": "string",
          "description": "The display name of a study procedure",
          "example": "North Star"
        },
        "identifier": {
          "type": "string",
          "description": "The identifier of a study procedure",
          "example": "NSTAR"
        },
        "label": {
          "$ref": "./DisplayName.json"
        }
      },
      "additionalProperties": false,
      "required": [
        "display_name",
        "identifier",
        "label"
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "account",
    "site",
    "study_event",
    "study_procedure",
    "subject"
  ]
}
