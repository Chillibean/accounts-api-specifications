      },
      "FormTemplate": {
        "$ref": "refs/components/schemas/FormTemplate.json"
      },
      "FormTemplateField": {
        "$ref": "refs/components/schemas/FormTemplateField.json"
      },
      "Permissions": {
        "$ref": "refs/components/schemas/Permissions.json"
      },
      "PlanOptions": {
