  "servers": [
    {
      "url": "http://chillipharm.test:3000/api/v1",
      "description": "Local development"
    },
    {
      "url": "https://qa.chillipharm.dev/api/v1",
      "description": "QA"
    },
    {
      "url": "https://chillipharm-staging.com/api/v1",
      "description": "Staging"
    },
    {
      "url": "https://chillipharm-uat.com/api/v1",
      "description": "UAT"
    },
    {
      "url": "https://www.chillipharm.com/api/v1",
      "description": "Production"
    }
  ],
  "paths": {
    "/accounts": {
      "$ref": "refs/paths/accounts.json"
    },
    "/accounts/{account_id}": {
      "$ref": "refs/paths/accounts_{account_id}.json"
    },
    "/accounts/{account_id}/assets/{asset_id}/location": {
      "$ref": "refs/paths/accounts_{account_id}_assets_{asset_id}_location.json"
    },
    "/accounts/{account_id}/branding": {
      "$ref": "refs/paths/accounts_{account_id}_brandings.json"
    },
    "/accounts/{account_id}/chapter_definitions": {
      "$ref": "refs/paths/accounts_{account_id}_chapter_definitions.json"
    },
    "/accounts/{account_id}/downloads/{download_request_uuid}": {
      "$ref": "refs/paths/accounts_{account_id}_downloads.json"
    },
    "/accounts/{account_id}/form_templates/{form_template_id}": {
      "$ref": "refs/paths/accounts_{account_id}_form_templates_{form_template_id}.json"
    },
    "/accounts/{account_id}/study_arms": {
      "$ref": "refs/paths/accounts_{account_id}_study_arms.json"
    },
    "/accounts/{account_id}/trial_configurations/{trial_configuration_id}/study_procedure_definitions": {
      "$ref": "refs/paths/accounts_{account_id}_trial_configurations_{trial_configuration_id}_study_procedure_definitions.json"
    },
    "/accounts/{account_id}/trial_configurations": {
      "$ref": "refs/paths/accounts_{account_id}_trial_configurations.json"
    },
    "/accounts/{account_id}/trial_configurations/{trial_configuration_id}/site_reporting": {
      "$ref": "refs/paths/accounts_{account_id}_trial_configurations_{trial_configuration_id}_site_reporting.json"
    },
    "/accounts/{account_id}/trial_configurations/{trial_configuration_id}/site_reporting/export": {
      "$ref": "refs/paths/accounts_{account_id}_trial_configurations_{trial_configuration_id}_site_reporting_export.json"
    }
  },
  "components": {
    "securitySchemes": {
      "userSessionKey": {
        "type": "apiKey",
        "in": "cookie",
        "name": "user_session_key",
        "description": "User session key cookie"
      },
      "chillipharmSession": {
        "type": "apiKey",
        "in": "cookie",
        "name": "_chillipharm_session",
        "description": "ChilliPharm session cookie"
      }
    },
    "schemas": {
      "Account": {
        "$ref": "refs/components/schemas/Account.json"
      },
