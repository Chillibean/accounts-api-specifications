        "additionalProperties": false,
        "required": [
          "id",
          "name",
          "permissions",
          "plan_options",
          "subject_id_settings",
          "timezone"
        ]
      },
      "BasicAccount": {
        "description": "An account",
        "type": "object",
        "properties": {
          "id": {
            "type": "number",
            "description": "The id of account",
            "example": 1
          },
          "name": {
            "type": "string",
