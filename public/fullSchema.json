        "properties": {
          "id": {
            "type": "number",
            "description": "The id of chapter definition",
            "example": 1
          },
          "name": {
            "type": "string",
            "description": "The name of chapter definition",
            "example": "pre"
          },
          "color": {
            "type": "string",
            "description": "The color of chapter definition",
            "example": "#ccc"
          },
          "position": {
            "type": "number",
            "description": "The position of chapter definition",
            "example": 1
          }
        },
        "additionalProperties": false,
        "required": [
          "id",
          "name",
          "color"
        ]
      },
      "ChapterDefinitions": {
        "type": "array",
        "items": {
          "$ref": "#/components/schemas/ChapterDefinition"
        }
      },
      "CompletedForm": {
        "description": "Completed form",
        "type": "object",
        "properties": {
          "id": {
            "type": "number",
            "description": "The id of a form",
            "example": 1
          },
          "name": {
            "type": "string",
            "description": "The name of a form",
            "example": "Form 1"
          }
        }
      },
      "Error": {
        "description": "An error",
        "type": "object",
        "properties": {
          "errors": {
            "type": "object",
            "properties": {
              "message": {
                "description": "The error message",
                "example": "Bad request. Missing or invalid input parameters."
              },
              "fields": {
                "description": "The error fields",
                "type": "object",
                "example": {
                  "name": [
                    "can't be blank"
                  ],
                  "color": [
                    "can't be blank"
                  ]
                }
              }
            }
          }
        },
        "additionalProperties": false,
        "required": [
          "errors"
        ]
      },
      "FormTemplate": {
        "description": "Form template field",
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "The field id",
            "example": "1"
          },
          "title": {
            "type": "string",
            "description": "The field title",
            "example": "Field title"
          },
          "version_number": {
            "type": "number",
            "description": "The version number of the form template",
            "example": 1
          }
        },
        "additionalProperties": false,
        "required": [
          "id",
          "title",
          "version_number"
        ]
      },
      "FormTemplateField": {
        "description": "Form template field",
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "The field id",
            "example": "1"
          },
          "title": {
            "type": "string",
            "description": "The field title",
            "example": "Field title"
          },
          "type": {
            "type": "string",
            "description": "The field type",
            "example": "single_line"
          },
          "input_guidance": {
            "type": "string",
            "description": "The field input guidance",
            "example": "guidance",
            "nullable": true
          },
          "enabled": {
            "type": "boolean",
            "description": "Whether the field is enabled",
            "example": true
          },
          "other_enabled": {
            "type": "boolean",
            "description": "Whether other types are enabled",
            "example": false
          },
          "value": {
            "oneOf": [
              {
                "type": "string",
                "example": ""
              },
              {
                "type": "array",
                "description": "The field value",
                "example": []
              }
            ]
          },
          "validations": {
            "type": "object",
            "properties": {
              "required": {
                "type": "boolean",
                "description": "Whether the field is required",
                "example": true
              },
              "input_format": {
                "type": "string",
                "description": "regex format for input",
                "example": "[a-zA-Z]{1}[a-zA-Z]{1}-\\d{1}\\d{1}\\d{1}\\d{1}\\d{1}",
                "nullable": true
              },
              "character_limit": {
                "type": "integer",
                "description": "The maximum characters",
                "example": 10,
                "nullable": true
              },
              "character_restriction": {
                "type": "string",
                "description": "restricted character type",
                "example": "letters_and_numbers",
                "nullable": true
              }
            },
            "required": [
              "required",
              "input_format",
              "character_limit",
              "character_restriction"
            ],
            "additionalProperties": false
          },
          "options": {
            "type": "array",
            "description": "The field options",
            "items": {
              "type": "string",
              "example": "option1"
            }
          },
          "comments": {
            "type": "array",
            "description": "The field comments",
            "items": {
              "type": "string",
              "example": "comment1"
            },
            "nullable": true
          }
        },
        "required": [
          "id",
          "title",
          "type",
          "enabled",
          "other_enabled",
          "value",
          "validations",
          "options"
        ],
        "additionalProperties": false
      },
      "FlaggedTask": {
        "description": "A flagged task",
        "type": "object",
        "properties": {
          "reason_for_creation": {
            "type": "string",
            "description": "Action required",
            "example": "Please complete xyz"
          },
          "created_at": {
            "type": "string",
            "description": "Created at date",
            "example": "October 15th, 2024"
          },
          "reason_for_resolving": {
            "type": "string",
            "description": "Action resolved",
            "example": "Completed xyz",
            "nullable": true
          },
          "resolved_at": {
            "type": "string",
            "description": "Resolved at date",
            "example": "October 15th, 2024",
            "nullable": true
          }
        }
      },
      "Permissions": {
        "type": "object",
        "properties": {
          "admin": {
            "type": "boolean",
            "description": "Whether the user is an admin",
            "example": true
          },
          "manager": {
            "type": "boolean",
            "description": "Whether the user is a manager",
            "example": false
          },
          "reporting": {
            "type": "boolean",
            "description": "Whether the user has reporting permissions",
            "example": true
          },
